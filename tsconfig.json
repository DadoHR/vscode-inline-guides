{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2020",
    "outDir": "dist",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "NodeNext",
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true
  }
}
